{
  "module": {
    "name": "component",
    "type": "feature",
    "description": "$string:module_desc",
    "mainElement": "ComponentAbility",
    "deviceTypes": [
      "phone",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      }
    ],
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "ComponentAbility",
        "srcEntry": "./ets/componentability/ComponentAbility.ts",
        "description": "$string:ComponentAbility_desc",
        "icon": "$media:icon",
        "label": "$string:ComponentAbility_label",
        "launchType": "singleton",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true
      }
    ]
  }
}